                <TextBox Grid.Row="2" Grid.Column="6" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{Binding character.Constitution}" MaxLength="3" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
