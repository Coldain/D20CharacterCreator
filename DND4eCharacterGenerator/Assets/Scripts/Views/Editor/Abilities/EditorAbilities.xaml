                <TextBox Name="WISCheck" Text="{Binding character.WisdomCheck}" Grid.Row="5" Grid.Column="9" VerticalAlignment="Center" HorizontalAlignment="Center" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
                <TextBox Name="CHACheck" Text="{Binding character.CharismaCheck}" Grid.Row="6" Grid.Column="9" VerticalAlignment="Center" HorizontalAlignment="Center" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
                <!--#endregion-->

            </Grid>
        </Viewbox>

