                <TextBox Grid.Row="4" Grid.Column="6" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{Binding character.Intelligence}" MaxLength="3" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
                <TextBox Grid.Row="5" Grid.Column="6" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{Binding character.Wisdom}" MaxLength="3" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
                <TextBox Grid.Row="6" Grid.Column="6" VerticalAlignment="Center" HorizontalAlignment="Right" Text="{Binding character.Charisma}" MaxLength="3" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
