                <!--#region Ability Descriptions-->
                <Label Content="{Binding abilities.SubList[3]}" Foreground="White" Grid.Row="1" Grid.Column="7" FontWeight="Bold"/>
                <Label Content="{Binding abilities.MainList[3]}" Foreground="White" Grid.Row="1" Grid.Column="7" FontSize="8" VerticalAlignment="Bottom" Margin="0,0,0,5" />
                <Label Content="{Binding abilities.SubList[0]}" Foreground="White" Grid.Row="2" Grid.Column="7" FontWeight="Bold"/>
                <Label Content="{Binding abilities.MainList[0]}" Foreground="White" Grid.Row="2" Grid.Column="7" FontSize="8" VerticalAlignment="Bottom" Margin="0,0,0,5" Padding="5,3,0,5" />
                <Label Content="{Binding abilities.SubList[1]}" Foreground="White" Grid.Row="3" Grid.Column="7" FontWeight="Bold"/>
                <Label Content="{Binding abilities.MainList[1]}" Foreground="White" Grid.Row="3" Grid.Column="7" FontSize="8" VerticalAlignment="Bottom" Margin="0,0,0,5" />
                <Label Content="{Binding abilities.SubList[5]}" Foreground="White" Grid.Row="4" Grid.Column="7" FontWeight="Bold"/>
                <Label Content="{Binding abilities.MainList[5]}" Foreground="White" Grid.Row="4" Grid.Column="7" FontSize="8" VerticalAlignment="Bottom" Margin="0,0,0,5" />
                <Label Content="{Binding abilities.SubList[4]}" Foreground="White" Grid.Row="5" Grid.Column="7" FontWeight="Bold"/>
                <Label Content="{Binding abilities.MainList[4]}" Foreground="White" Grid.Row="5" Grid.Column="7" FontSize="8" VerticalAlignment="Bottom" Margin="0,0,0,5" />
                <Label Content="{Binding abilities.SubList[2]}" Foreground="White" Grid.Row="6" Grid.Column="7" FontWeight="Bold"/>
                <Label Content="{Binding abilities.MainList[2]}" Foreground="White" Grid.Row="6" Grid.Column="7" FontSize="8" VerticalAlignment="Bottom" Margin="0,0,0,5" />
                <!--#endregion-->

                <!--#region Ability Mods-->
                <TextBox Name="STRMod" Text="{Binding character.StrengthMod}" Grid.Row="1" Grid.Column="8" VerticalAlignment="Center" HorizontalAlignment="Center" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
                <TextBox Name="CONMod" Text="{Binding character.ConstitutionMod}" Grid.Row="2" Grid.Column="8" VerticalAlignment="Center" HorizontalAlignment="Center" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
                <TextBox Name="DEXMod" Text="{Binding character.DexterityMod}" Grid.Row="3" Grid.Column="8" VerticalAlignment="Center" HorizontalAlignment="Center" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
                <TextBox Name="INTMod" Text="{Binding character.IntelligenceMod}" Grid.Row="4" Grid.Column="8" VerticalAlignment="Center" HorizontalAlignment="Center" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
                <TextBox Name="WISMod" Text="{Binding character.WisdomMod}" Grid.Row="5" Grid.Column="8" VerticalAlignment="Center" HorizontalAlignment="Center" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
                <TextBox Name="CHAMod" Text="{Binding character.CharismaMod}" Grid.Row="6" Grid.Column="8" VerticalAlignment="Center" HorizontalAlignment="Center" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/> 

                <!--#region Ability Mods + 1/2 level-->
                <TextBox Name="STRCheck" Text="{Binding character.StrengthCheck}" Grid.Row="1" Grid.Column="9" VerticalAlignment="Center" HorizontalAlignment="Center" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
                <TextBox Name="CONCheck" Text="{Binding character.ConstitutionCheck}" Grid.Row="2" Grid.Column="9" VerticalAlignment="Center" HorizontalAlignment="Center" Width="19" IsHitTestVisible="False" Focusable="False" HorizontalContentAlignment="Center"/>
